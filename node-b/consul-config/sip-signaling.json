{
  "service": {
    "name": "sip-signaling",
    "port": 13024,
    "tags": ["onprem", "prod", "udp"],
    "checks": [
      {
        "name": "SIP Signaling UDP Port Check",
        "args": ["/bin/sh", "-c", "echo 'HEALTH' | nc -u -w 2 sip-signaling 13024"],
        "interval": "10s",
        "timeout": "5s"
      }
    ]
  }
}