{
  "service": {
    "name": "sip-gateway",
    "port": 5060,
    "tags": ["gcp", "udp"],
    "checks": [
      {
        "name": "SIP Gateway UDP Port Check",
        "args": ["/bin/sh", "-c", "echo 'HEALTH' | nc -u -w 2 sip-gateway 5060"],
        "interval": "10s",
        "timeout": "5s"
      }
    ]
  }
}